{
  "manifest_version": 3,
  "name": "Keka Extension",
  "version": "1.1",
  "permissions": ["tabs", "storage", "scripting"],
  "action": {
    "default_title": "Keka Hours",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://techahead.keka.com/*"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["https://techahead.keka.com/*"]
}
